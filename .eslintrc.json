{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "camelcase": 0,
    "no-unused-vars": 0,
    "global-require": 0,
    "dot-notation": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-nested-ternary": 0
  }
}